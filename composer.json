{
    "name": "drupal/webauthn",
    "description": "This module aims to support the upcoming \"Web Authentication\" protocol, that leverages Public Key Credentials to provide password-less login along with stronger security.",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Dennis A. Torres Rodriguez",
            "email": "d70rr3s@gmail.com"
        }
    ],
    "homepage": "https://www.drupal.org/project/webauthn",
    "support": {
        "issues": "https://www.drupal.org/project/issues/webauthn",
        "source": "https://git.drupalcode.org/project/webauthn"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true
    },
    "require": {
        "drupal/core": "^8.8",
        "nyholm/psr7": "^1.2",
        "web-auth/webauthn-lib": "^3.0"
    }
}
